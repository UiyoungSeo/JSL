
public class Ex09 {

	public static void main(String[] args) {
		// Lotto 1~45, 중복x 여섯 개
		
		int[] arr = new int[6];
		
		for (int i = 0; i < arr.length; i++) {
			arr[i] = createNumber();
			System.out.println(arr[i]);
		}
		
	}

	static int createNumber() {
		return (int) (Math.random() * 45 + 1);
	}

}
