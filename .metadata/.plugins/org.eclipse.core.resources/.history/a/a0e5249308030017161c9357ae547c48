import java.util.Scanner;

public class Ex08 {

	public static void main(String[] args) {
		// 다섯개의 정수를 입력받아서 그것의 합을 구하시오

		Scanner sc = new Scanner(System.in);
		System.out.print("input 5 integers >>");
		int sum = 0;
		for (int i = 0; i < 5; i++) {
			sum += sc.nextInt();
		}
		
		System.out.println("sum : " + sum);
	}

}
