
public class Ex05 {

	public static void main(String[] args) {
		System.out.println("******* multiplication table ******* \n");

		for (int i = 1; i <= 9; i++) {
			for (int j = 1; j <= 9; j++) {
				System.out.println();
				System.out.print(i + "*" + j + "=" + i * j);
				System.out.print("\t");
			}
		}
	}

}
