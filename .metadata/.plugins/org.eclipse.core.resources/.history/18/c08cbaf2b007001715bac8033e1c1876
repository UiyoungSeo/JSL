public class Goods {
	String name;
	int price;
	int sold;	//팔린 갯수

	public static void main(String[] args) {
		Goods tv = new Goods();
		tv.name = "LG SUTV";
		tv.price = 500000;
		tv.sold = 10;
		
		Goods camera = new Goods();
		camera.name = "nikon";
		camera.price = 450000;
		camera.sold = 5;
		
		System.out.println("상품이름 : " +tv.name);
				
	}

}
