import java.util.Scanner;

public class Ex03 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("원하는 숫자를 입력하세요 >>");
		int input = sc.nextInt();

		for (int i = input; i <= input; i--) {
			for (int j = 1; j <= i; j++) {
				System.out.print("*");
			}
		}
	}

}
