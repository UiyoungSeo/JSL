
public class Ex05 {

	public static void main(String[] args) {
		System.out.print("******* multiplication table ******* \n");

		while (true) {

			for (int i = 1; i <= 9; i++) {
				System.out.println();
				for (int j = 1; j <= 9; j++) {
					System.out.print(i + "*" + j + "=" + i * j);
					System.out.print("\t");
				}
			}
			System.out.println("계속? (y/n)");
		}

	}

}
