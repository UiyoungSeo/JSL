import java.util.Arrays;
import java.util.Random;

public class Ex02 {

	public static void main(String[] args) {
		Random ran = new Random();
		int[][] score = new int[4][5];
		for (int i = 0; i < 4; i++) {
			for (int j = 0; j < 5; j++) {
				score[i][j] = ran.nextInt(10);
			}
		}
		
		System.out.println(Arrays.toString(score));
	}

}
